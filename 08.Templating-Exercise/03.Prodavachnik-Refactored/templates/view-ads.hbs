<section id="viewAds" class="viewAds">
    <h1 class="titleForm">Advertisements</h1>
    <div id="ads" class="ads">
        <table>
            <tr>
                <th>Title</th>
                <th>Publisher</th>
                <th>Description</th>
                <th>Price</th>
                <th>Date Published</th>
                <th>Actions</th>
            </tr>
            {{#each ads}}
                <tr>
                    <th>{{Title}}</th>
                    <th>{{Publisher}}</th>
                    <th>{{Description}}</th>
                    <th>{{Price}}</th>
                    <th>{{Date}}</th>
                    <th>
                        <a href="#">[Delete]</a>
                        <a href="#">[Edit]</a>
                    </th>
                </tr>
            {{/each}}
        </table>
    </div>
</section>